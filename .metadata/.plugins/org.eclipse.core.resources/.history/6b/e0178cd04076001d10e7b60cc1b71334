	public void addVoter(Voter voter) {
		this.voter.add(voter);
	}
	
	public void addpresident(Politician president) {
		this.president.add(president);
	}

	public void addGovernor(Politician governor) {
		this.governor.add(governor);
	}

	public void setSenator(Politician senator) {
		this.senator.add(senator);
	}

	public void setCongressman(Politician congressman) {
		this.congressman.add(congressman);
	}

	public void setStateRepresentative(Politician stateRepresentative) {
		this.stateRepresentative.add(stateRepresentative);
	}	
}
