package application;

import javax.swing.JOptionPane;

import assets.Voter;

public class Program {
	
	public static void main(String[] args) {
		
		JOptionPane.showMessageDialog(null, "Hello Citizen!");
		String name = JOptionPane.showInputDialog("Enter your name to proceed");
		int age = Integer.parseInt(JOptionPane.showInputDialog("Now enter your age"));
		Voter voter = new Voter(name, age);
		
		JOptionPane.showMessageDialog(null, voter.toString());
	}
}