package assets;

import assetsEnum.VotingCapacity;

public class Voter {
	
	private String name;
	private int age;
	private VotingCapacity votingCapacity;
	
	public Voter() {}

	public String getName() {
		return name;
	}

	public void setName(String name) {
		this.name = name;
	}

	public int getAge() {
		return age;
	}

	public void setAge(int age) {
		this.age = age;
	}

	public VotingCapacity getVotingCapacity() {
		return votingCapacity;
	}

	public void setVotingCapacity(VotingCapacity votingCapacity) {
		String votingCapacity = "Cant_vote";
		if ((age < 18 && age >= 16) || (age > 70)) {
			
			this.votingCapacity = Optional_vote";
		}
	};
	
	

}
